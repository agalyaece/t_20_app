<p>Selected players</p>
        {isLoading ? (
          <p>Loading...</p>
        ) : error ? (
          <p>Error fetching data: {error.message}</p>
        ) : (
          <div>
            {selectedPlayers.map((item, index) => (
              <div key={index}>
                <h2>{item.country_1} vs {item.country_2}</h2>
                {(item.teams).map((team, teamIndex) => (
                  
  
                  <div key={teamIndex}>
                    
                    <ul>
                      {(team.players).map((player, playerIndex) => (
                        <li key={playerIndex}>
                          <p>Team {playerIndex+1}</p>
                          {player.length > 0 ? (
                            player.map((innerPlayer, innerPlayerIndex) => (
                              <span key={innerPlayerIndex}>
                                
                                {/* Access properties of the innerPlayer object here */}
                                {innerPlayer.player_name_x} {/* Assuming a property for player name */}
                                {/* Access other player properties as needed */}
                              </span>
                            ))
                          ) : (
                            <p>No players for this team.</p>
                          )}
                        </li>
                      ))}
  
  
                    </ul>
                  </div>
                ))}
              </div>
            ))}
          </div>
        )}